<div class="owner-form-wrapper">
  <div class="form">
    <div class="wrapper">
      <h1>Shelter/Foster Profile Information:</h1>
      <%= form_for @owner, url: { action: "create" } do |f| %>
        <%= f.label :name %>
        <%= f.text_field :name %>
        <%= f.label :phone %>
        <%= f.text_field :phone %>
        <%= f.label :address %>
        <%= f.text_field :address %>
        <%= f.label :second_address %>
        <%= f.text_field :second_address %>
        <%= f.label :city %>
        <%= f.text_field :city %>
        <%= f.label :state %>
        <%= f.text_field :state %>
        <%= f.label :zip %>
        <%= f.text_field :zip %>
        <%= f.label :profile_image %>
        <%= f.file_field :profile_image %>
        <%= f.label :hero_image %>
        <%= f.file_field :hero_image %>
        <%= f.label :email %>
        <%= f.text_field :email %>
        <%= f.label :website %>
        <%= f.text_field :website %>
        <%= f.label :facebook_url %>
        <%= f.text_field :facebook_url %>
        <%= f.label :twitter_url %>
        <%= f.text_field :twitter_url %>
        <%= f.label :instagram_url %>
        <%= f.text_field :instagram_url %>
        <%= f.hidden_field :user_id, :value => current_user.id %>
        <%= f.submit 'Create' %>
      <% end %>
    </div>
  </div>
</div>